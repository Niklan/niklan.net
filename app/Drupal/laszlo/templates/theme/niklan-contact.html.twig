{#
/**
 * @file
 * Default theme implementation to present contact page contents.
 *
 * Available variables:
 * - description: The renderrable array with description.
 * - email: The email address.
 * - telegram: The Telegram URL.
 */
#}
{% embed 'laszlo:author-page' %}
  {% block children %}
    {% include 'laszlo:contact' with {
      description,
      email,
      telegram,
    } only %}
  {% endblock %}
{% endembed %}