{#
/**
 * @file
 * Default theme implementation for an 'input' #type form element.
 *
 * Available variables:
 * - attributes: A list of HTML attributes for the input element.
 * - children: Optional additional rendered elements.
 *
 * @see template_preprocess_input()
 * @see laszlo_preprocess_input()
 *
 * @ingroup themeable
 */
#}
{%- embed 'laszlo:text-input' with {
  input_attributes: attributes,
  start_decorator,
  end_decorator,
} only %}
  {% block start_decorator -%}
    {%- if start_decorator -%}
      {{- start_decorator -}}
    {%- endif -%}
  {%- endblock %}

  {% block end_decorator -%}
    {%- if end_decorator -%}
      {{- end_decorator -}}
    {%- endif -%}
  {%- endblock %}
{% endembed -%}
{{- children }}
