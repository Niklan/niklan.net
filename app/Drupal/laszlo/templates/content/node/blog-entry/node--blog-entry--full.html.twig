{% embed 'laszlo:article' with {
  title: node.label(),
  url,
  url_absolute,
  published_timestamp,
  estimated_read_time,
  comment_count,
  content,
  toc_links,
  poster_uri,
  attachments,
  } only %}
  {% block summary %}{{ content.body }}{% endblock %}
  {% block content %}{{ content.external_content }}{% endblock %}
  {% block comments %}{{ dd(content) }}{% endblock %}
{% endembed %}
