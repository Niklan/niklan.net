{
    "name": "niklan/project",
    "description": "Project for blog.",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://niklan.net",
    "authors": [
        {
            "name": "Nikita Malyshev (Niklan)",
            "homepage": "https://niklan.net",
            "email": "i@niklan.net",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "path",
            "url": "web/modules/custom/*"
        },
        {
            "type": "path",
            "url": "web/themes/custom/*"
        }
    ],
    "require": {
        "php": ">=8.1",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal/antibot": "^1.2",
        "drupal/contact_emails": "^1.14",
        "drupal/contact_storage": "^1.0@beta",
        "drupal/contact_tools": "^1.0",
        "drupal/core-composer-scaffold": "^9.0.0",
        "drupal/core-recommended": "^9.0.0",
        "drupal/easy_breadcrumb": "^1.8",
        "drupal/extra_field": "^1.1",
        "drupal/glisseo": "1.x-dev",
        "drupal/google_tag": "^1.0",
        "drupal/imagemagick": "^3.0",
        "drupal/metatag": "^1.1",
        "drupal/paragraphs": "^1.2",
        "drupal/pathauto": "^1.0",
        "drupal/photoswipe": "^3.0",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/redirect": "^1.0@alpha",
        "drupal/search_api": "^1.8",
        "drupal/simple_sitemap": "^3.0",
        "drupal/smtp": "^1.0@beta",
        "drupal/taxonomy_custom_controller": "^1.0",
        "drupal/token": "^1.0",
        "drupal/twig_tweak": "^3.0",
        "drush/drush": "^11.0",
        "niklan/niklan": "^1.0",
        "weitzman/drupal-test-traits": "^1.5"
    },
    "require-dev": {
        "chi-teck/drupal-coder-extension": "^1.0",
        "drupal/core-dev": "^9.3",
        "mglaman/drupal-check": "^1.3",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Niklan\\Tests\\": "tests/src"
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "gitignore": false,
            "locations": {
                "web-root": "./web",
                "libraries-root": "./web/libraries"
            },
            "file-mapping": {
                "[web-root]/sites/default/settings.all.php": "assets/scaffold/settings.all.php",
                "[web-root]/sites/default/settings.local.php": {
                    "path": "assets/scaffold/settings.local.php",
                    "overwrite": false
                },
                "[web-root]/sites/default/settings.php": {
                    "path": "web/core/assets/scaffold/files/default.settings.php",
                    "append": "assets/scaffold/settings-php-additions.txt",
                    "overwrite": false
                },
                "[web-root]/sites/development.services.yml": "assets/scaffold/development.services.yml",
                "[web-root]/robots.txt": {
                    "append": "assets/scaffold/robots-txt-additions.txt"
                },
                "[libraries-root]/photoswipe/dist/default-skin/default-skin.css": "assets/vendor/photoswipe/dist/default-skin/default-skin.css",
                "[libraries-root]/photoswipe/dist/default-skin/default-skin.png": "assets/vendor/photoswipe/dist/default-skin/default-skin.png",
                "[libraries-root]/photoswipe/dist/default-skin/default-skin.svg": "assets/vendor/photoswipe/dist/default-skin/default-skin.svg",
                "[libraries-root]/photoswipe/dist/default-skin/preloader.gif": "assets/vendor/photoswipe/dist/default-skin/preloader.gif",
                "[libraries-root]/photoswipe/dist/photoswipe.css": "assets/vendor/photoswipe/dist/photoswipe.css",
                "[libraries-root]/photoswipe/dist/photoswipe.min.js": "assets/vendor/photoswipe/dist/photoswipe.min.js",
                "[libraries-root]/photoswipe/dist/photoswipe-ui-default.min.js": "assets/vendor/photoswipe/dist/photoswipe-ui-default.min.js",
                "[libraries-root]/photoswipe/package.json": "assets/vendor/photoswipe/package.json",
                "[libraries-root]/prismjs/prism.js": "assets/vendor/prismjs/prism.js"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
        }
    },
    "scripts": {
        "phpcs": "vendor/bin/phpcs -ps",
        "phpcbf": "vendor/bin/phpcbf",
        "phpstan": "vendor/bin/phpstan analyse",
        "phpunit": "sudo -u root -E sudo -u www-data -E vendor/bin/phpunit",
        "phpunit-coverage": "sudo -u root -E sudo -u www-data -E vendor/bin/phpunit --coverage-text"
    }
}
